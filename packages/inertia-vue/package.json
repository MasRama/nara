{
  "name": "@nara-web/inertia-vue",
  "version": "1.0.2",
  "description": "Vue 3 + Inertia.js adapter for NARA",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@inertiajs/vue3": "^2.0.0"
  },
  "peerDependencies": {
    "@nara-web/core": "^1.0.0",
    "vue": "^3.5.0",
    "vite": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@nara-web/core": "^1.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vue": "^3.5.13"
  },
  "license": "MIT"
}
